{
  "name": "@vibe-kit/mcp-server",
  "version": "0.0.1",
  "description": "MCP server for VibeKit development workflow",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "vibekit-mcp": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "mcp",
    "vibekit",
    "model-context-protocol",
    "ai",
    "development-tools"
  ],
  "author": "VibeKit Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/superagent-ai/vibekit.git",
    "directory": "packages/mcp-server"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "start:stdio": "node dist/index.js",
    "start:http": "node dist/index.js --transport http-stream --port 8080",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "@vibe-kit/projects": "^0.0.1",
    "fastmcp": "^1.27.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@vitest/coverage-v8": "^3.1.3",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^3.1.3"
  }
}
