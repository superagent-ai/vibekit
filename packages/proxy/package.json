{
  "name": "@vibekit/proxy",
  "version": "0.1.0",
  "description": "TypeScript + Hono LLM Proxy with secret redaction",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "start": "tsx src/working-proxy.ts",
    "minimal": "tsx src/minimal-proxy.ts",
    "serve": "npm run start"
  },
  "keywords": [
    "proxy",
    "llm",
    "security",
    "redaction",
    "secrets",
    "hono"
  ],
  "dependencies": {
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "detect-secrets": "^1.0.6",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "fast-redact": "^3.5.0",
    "http-proxy-middleware": "^3.0.0",
    "js-yaml": "^4.1.0",
    "pino": "^9.6.0",
    "redact-pii": "^3.4.0",
    "redact-secrets": "^1.0.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/uuid": "^10.0.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
