{
  "name": "@vibe-kit/preview",
  "version": "0.0.1",
  "description": "Dev server preview system for VibeKit projects",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "vibekit",
    "preview",
    "dev-server",
    "development",
    "framework-detection"
  ],
  "author": "VibeKit Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/superagent-ai/vibekit.git",
    "directory": "packages/preview"
  },
  "homepage": "https://github.com/superagent-ai/vibekit#readme",
  "dependencies": {
    "@vibe-kit/logger": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^2.1.8",
    "tsup": "^8.0.1",
    "typescript": "^5.8.2",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}