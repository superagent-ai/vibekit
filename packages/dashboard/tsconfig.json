{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@vibe-kit/logger": ["../logger/dist/index.d.ts", "../logger/src/index.ts"],
      "@vibe-kit/logger/*": ["../logger/dist/*", "../logger/src/*"],
      "@vibe-kit/mcp-client": ["../mcp-client/dist/index.d.ts", "../mcp-client/src/index.ts"],
      "@vibe-kit/mcp-client/*": ["../mcp-client/dist/*", "../mcp-client/src/*"],
      "@vibe-kit/projects": ["../projects/dist/index.d.ts", "../projects/src/index.ts"],
      "@vibe-kit/projects/*": ["../projects/dist/*", "../projects/src/*"],
      "@vibe-kit/taskmaster": ["../taskmaster/dist/index.d.ts", "../taskmaster/src/index.ts"],
      "@vibe-kit/taskmaster/*": ["../taskmaster/dist/*", "../taskmaster/src/*"],
      "@vibe-kit/ai-chat": ["../ai-chat/dist/index.d.ts", "../ai-chat/src/index.ts"],
      "@vibe-kit/ai-chat/*": ["../ai-chat/dist/*", "../ai-chat/src/*"],
      "@vibe-kit/auth": ["../auth/dist/index.d.ts", "../auth/src/index.ts"],
      "@vibe-kit/auth/*": ["../auth/dist/*", "../auth/src/*"],
      "@vibe-kit/sdk": ["../sdk/dist/index.d.ts", "../sdk/src/index.ts"],
      "@vibe-kit/sdk/*": ["../sdk/dist/*", "../sdk/src/*"],
      "@vibe-kit/dagger": ["../dagger/dist/index.d.ts", "../dagger/src/index.ts"],
      "@vibe-kit/dagger/*": ["../dagger/dist/*", "../dagger/src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
